        <link href="{{ asset('assetsClient/css/style.css') }}" rel="stylesheet">
        <style>
          body {
            background-image: url("../assetsClient/img/test.jpg") ;
            background-size: cover;
            background-position: center;
        }
        .form-box {
          position: relative; /* Ensure positioning context */
          max-width: 400px; /* Set maximum width */
          margin: 0 auto; /* Center horizontally */
          padding: 20px; /* Padding */
          border: 1px solid #ccc; /* Border */
          border-radius: 5px; /* Border radius */
          background-color: #f9f9f9; /* Background color */
      }
  
      .exit-button {
        position: absolute; /* Position absolutely */
        top: 5px; /* Distance from top */
        right: 5px;
          cursor: pointer;

      }
      .button {
        background-color: transparent;
        color: black;
        border: 0.002px solid #1a0f00;
        width: auto; /* Set the width */
        height: 50px;
    
      
      
        margin-top: 20px;
        margin-left: 100px;  /* Add margin to the top */
        .button:hover {background-color: #e7e7e7;
          text-color:black;}
    }
  </style>
        {% endblock %}

        {% block javascripts %}
